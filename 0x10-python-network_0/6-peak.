#!/usr/bin/python3
""" peak in a list of unsorted integers
"""

def find_peak(list_of_integers):
"""
    Args:
list_of_integers(int): list of integers to find peak of
Returns: peak of list_of_integers or None
"""
	size = len(list_of_integers)
	mid_e = size
	mid = size // 2
	if size == 0:
		return None

